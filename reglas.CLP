;;Definimos el template que recibira la informacion para  identificar los parametros que mediran el peligro que enfrentará el usuario en una reunión(deftemplate peligro_reunion 		(slot zona)		(slot num_personas)		(slot tipo_de_reunion)		(slot familia_cont))	;;Se define el template que guardara el mensaje de salida(deftemplate probabilidad		(slot mensaje));;Se definen las 48 reglas divididas en 4 preguntas, mostrando la opcion por número según lo que se esté trabajando. ;;El primer número representa la zona 1(abierta)  2 (cerrada);;El segundo número representa el número de personas en la reunión 1(1-10 personas)  2 (11-20 personas)  3(+20 personas);;El tercer número representa el tipo de personas con las que se tendrá la reunión 1(familiar)  2(laboral) 3(amigos) 4(de_desconocidos);;El cuarto número representa si el anfitrión de la fiesta tiene /tuvo familiares contagiados  1(sí)  2 (no)(defrule opcion1-1-1-1	(peligro_reunion		(zona zona_abierta)		(num_personas 1_10_personas)		(tipo_de_reunion familiar)		(familia_cont si))	=> 	(printout t crlf "El peligro de contagio es de 0.4 (Medio)" crlf crlf)		(assert (probabilidad (mensaje "El peligro de contagio es de 0.4 (Medio)")))	)(defrule opcion1-1-1-2	(peligro_reunion		(zona zona_abierta)		(num_personas 1_10_personas)		(tipo_de_reunion familiar)		(familia_cont no))	=> 	(printout t crlf "El peligro de contagio es de 0.25 (Bajo)" crlf crlf)		(assert (probabilidad (mensaje "El peligro de contagio es de 0.25 (Bajo)"))))(defrule opcion1-1-2-1	(peligro_reunion		(zona zona_abierta)		(num_personas 1_10_personas)		(tipo_de_reunion laboral)		(familia_cont si))	=> 	(printout t crlf "El peligro de contagio es de 0.33 (Medio)" crlf crlf)		(assert (probabilidad (mensaje "El peligro de contagio es de 0.33 (Medio)"))))(defrule opcion1-1-2-2	(peligro_reunion		(zona zona_abierta)		(num_personas 1_10_personas)		(tipo_de_reunion laboral)		(familia_cont no))	=> 	(printout t crlf "El peli